<!DOCTYPE html>
<html lang="pt-br">

<%- include partials/head %>

    <head>
        <link rel='stylesheet' href='/stylesheets/main.css'>
        <title>DOAME</title>
    </head>

    <body>

        <%- include partials/header %>
            <main>
                <section class="projects">
                    <div class="container">
                        <% for( let i = 0; i < listaParceiros.length; i++ ) { %>
                            <% let parceiro = listaParceiros[i]; %>
                                <article>
                                    <div class="thumb" style="background-image: url('<%= parceiro.imagem %>')"></div>
                                    <span class="titulo-projeto"><%= parceiro.nome %></span>
                                    <div class="info">
                                        <p>
                                            <%= parceiro.descricao %>
                                        </p>
                                        <img class="querodoar" src="/images/logo/querodoar.png">
                                    </div>
                                </article>
                                <% } %>
                    </div>
                </section>
            </main>
            <%- include partials/footer %>
                <script src="/javascripts/main.js"></script>
    </body>

</html>